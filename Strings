// LogicalOperationCombine.pp: Сравнение двух строк с поиском наименьшей

#include "stdafx.h"
#include <iostream>
#include <math.h>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

int main() {
	string a,b,c;
	cin >> a >> b >> c;
	if (a < b && a < c)	cout << a;
	else if (b < a && b < c) cout << b;
	else cout << c;
	
	getchar();
	getchar();
	return 0;
}
